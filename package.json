{
  "name": "bleaum",
  "productName": "Bleaum",
  "version": "1.0.0",
  "description": "Bleaum Desktop Application",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "build": "electron-forge make",
    "build:win": "electron-forge make --platform=win32",
    "build:mac": "electron-forge make --platform=darwin",
    "build:lin": "electron-forge make --platform=linux",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "Bleaum",
    "email": "comms@bleaum.io"
  },
  "build": {
    "publish": {
      "provider": "github",
      "owner": "mahfuz-alam0",
      "repo": "auto"
    },
    "win": {
      "target": "squirrel"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.7.0",
    "@electron-forge/maker-deb": "^7.7.0",
    "@electron-forge/maker-dmg": "^7.7.0",
    "@electron-forge/maker-rpm": "^7.7.0",
    "@electron-forge/maker-squirrel": "^7.7.0",
    "@electron-forge/maker-zip": "^7.7.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.7.0",
    "@electron-forge/plugin-fuses": "^7.7.0",
    "@electron-forge/plugin-vite": "^7.7.0",
    "@electron/fuses": "^1.8.0",
    "@types/antd": "^0.12.32",
    "@types/electron": "^1.4.38",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-toastify": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "electron": "34.3.0",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.31.0",
    "sass": "^1.85.1",
    "sass-embedded": "^1.85.1",
    "ts-node": "^10.9.2",
    "typescript": "~4.5.4",
    "vite": "^5.4.14"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "antd": "^5.24.2",
    "axios": "^1.8.1",
    "electron-log": "^5.4.0",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.6.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-loading-skeleton": "^3.5.0",
    "react-router-dom": "^7.2.0",
    "react-to-print": "^3.0.5",
    "socket.io-client": "^4.8.1"
  }
}